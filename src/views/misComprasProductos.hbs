{{> headerIDLanding }}

<div class="page-holder">
    <!-- navbar-->
    <div class="barranav">
        {{> navbarId}}
    </div>

    <body style="font-size: 15px;">
        <div class="container py-3 ContenerProdu">
            <div class="row ">

                {{#each productos}}
                <div class="card container producCliente">
                    <h4>{{nombre}}</h4>
                    <div class="row">
                        <div class="col-md-6">
                            <img src="/assets/img/Productos/{{imagen}}" alt="" >
                        </div>

                        <div class="col-md-6 TextProd">
                            <p>Precio: {{precio}}
                                <br>Unidades: {{Unidad}}
                            </p>
                        </div>
                    </div>

                </div>

                <br>
                {{/each}}

                <br>
            </div>
            <br>
        </div>
    </body>
    <br><br><br><br><br><br>
</div>
{{> footerId}}


