    <!-- navbar-->
    <div class="barranav">
        {{> navbarId}}
    </div>

    <body style="font-size: 15px;">
        <div class="container py-3">
            <div class="row align-content-center align-items-center justify-content-betwen">

                {{#each productos}}
                <div class="card producCliente">
                    <h4>{{nombre}}</h4>
                    <div class="col-mb-6" style="border:2px solid pink;">
                        <img src="/assets/img/Productos/{{imagen}}" alt="">
                    </div>
                    <div class="col-mb-6" style="border: 2px solid blueviolet;">
                        <p>Precio: {{precio}}
                            <br>Unidades: {{Unidad}}
                        </p>
                    </div>


                </div>
                <br>
                {{/each}}

                <br>
            </div>
            <br>
        </div>
    </body>
</div>
