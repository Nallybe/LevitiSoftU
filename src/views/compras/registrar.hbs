                <input type="text" class="form-control form-control-sm" list="Proveedores_Registrados"
                  style="border-radius: 0;" id="proveedor" name="proveedor" autocomplete="off"
                  oninput="val_proveedor()">
                <datalist id="Proveedores_Registrados">
                  {{#each proveedores}}
                  <option value="{{this}}">
                    {{/each}}
                </datalist>
                <div id="mensajeErrorProveedor"></div>
                <!--End Input Proveedor-->


                <!--Input Recibo-->
                <label class="form-label mt-2" for="recibo">Recibo</label>
                <input type="text" class="form-control form-control-sm" style="border-radius: 0;" id="recibo"
                  name="recibo" oninput="val_recibo()" autocomplete="off">
                <div id="mensajeErrorRecibo"></div>
                <!--End Input Recibo-->

                <!--Input Fecha Recibo-->
                <label class="form-label mt-2" for="fechaRecibo">Fecha Recibo</label>
                <input type="date" class="form-control form-control-sm mb-2" style="border-radius: 0;" id="fechaRecibo" name="fechaRecibo"
                  oninput="val_f_recibo()" autocomplete="off">
                <div id="mensajeErrorFechaRecibo"></div>
                <!--End Input Fecha Recibo-->

              </div>
            </div>
            <!--End Card #1-->

          </div>
          <div class="col-9">

            <!--Start Card #2-->

            <div class="card" style="border-radius: 0; width: 100%;">
              <div class="card-body">
                <div class="row">
                  <div class="col-md-11">
                    <h5 class="card-title">Agregar insumo</h5>

                    <div class="row">
                      <div class="col-md-4">
                        <!--Input Insumo-->
                        <label class="form-label" for="idInsumo_R">Insumo</label>
                        <input type="text" class="form-control form-control-sm" list="list_insumos"
                          style="border-radius: 0;" id="idInsumo_R" name="idInsumo_R" autocomplete="off"
                          oninput="val_ins()">
                        <datalist id="list_insumos">
                          {{#each insumos}}
                          <option value="{{nombre}}" medida="{{medida}}" id="{{idInsumo}}">nombre({{nombre}})
                            medida({{medida}})
                            stock({{stock}})</option>
                          {{/each}}
                        </datalist>
                        <div id="mensajeErrorInsumo"></div>
                        <!--End Input Insumo-->
                      </div>
                      <div class="col-md-3">
                        <!--Input Cantidad-->
                        <label class="form-label" for="cantidad_R">Cantidad</label>
                        <input type="number" class="form-control form-control-sm" style="border-radius: 0;"
                          id="cantidad_R" name="cantidad_R" autocomplete="off" oninput="val_cant()">
                        <div id="mensajeErrorCantidad"></div>
                        <!--End Input Cantidad-->
                      </div>
                      <div class="col-md-3">
                        <!--Input Precio-->
                        <label class="form-label" for="precio_R">Precio</label>
                        <input type="number" class="form-control form-control-sm" style="border-radius: 0;"
                          id="precio_R" name="precio_R" autocomplete="off" oninput="val_precio()">
                        <div id="mensajeErrorPrecio"></div>
                        <!--End Input Precio-->
                      </div>
                      <div class="col-md-2">

